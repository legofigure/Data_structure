#include <stdio.h>
#include <math.h>

int Min(int a,int b){
    if(a<b){
        return a;
    }
    else{
        return b;
    }
}

int main() {
    int n;
    int dp[50001];
    
    dp[0]=0;
    dp[1]=1;
    
    scanf("%d",&n);
    for(int i=1;i<224;i++)
		dp[i*i]=1;
    
    for(int i=2;i<=50000;i++){
        if(dp[i]!=1){
            dp[i]=4;
            for(int j=1;j<=sqrt(i);j++){
                dp[i]=Min(dp[i],dp[j*j]+dp[i-j*j]);
            }
        }
    }
    
    printf("%d",dp[n]);


    
    return 0;
}
