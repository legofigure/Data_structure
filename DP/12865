#include <stdio.h>

int dp[100001][101]={0,};

int Max(int a,int b){
    if(a>b){
        return a;
    }
    else{
        return b;
    }
}

int main() {
    int n,k;
    int weight[101]={0,};
    int value[101]={0,};
    
    scanf("%d %d",&n,&k);

    for(int i=0;i<n;i++){
        scanf("%d %d",&weight[i],&value[i]);
    }
    
    for(int i=0;i<n;i++){
        for(int j=0;j<=k;j++){
            if(weight[i]>j){
                dp[j][i]=dp[j][i-1];
            }
            else if(weight[i]<=j){
                dp[j][i]=Max(dp[j][i-1],dp[j-weight[i]][i-1]+value[i]);
            }            
        }
    }

    printf("%d ",dp[k][n-1]);
    
    return 0;
}
