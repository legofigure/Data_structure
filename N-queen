#include <stdio.h>

int arr[15][15]={0,};
int n,ans=0;
   
int check(int i, int j){
    for(int k=1;k<=n;k++){
        for(int l=1;l<=n;l++){
            if(i==k || j==l){
                if(arr[k][l]==2){
                    return 1;
                }
            }
        }
    }
    for(int k=i+1,l=1;k<=n;k++,l++){
        if(j-l>0){
            if(arr[k][j-l]==2){
                return 1;
            }
        }
        if(j+1<=n){
            if(arr[k][j+l]==2){
                return 1;
            }
        }
    }
    for(int k=i-1,l=1;k>=1;k--,l++){
        if(j-l>0){
            if(arr[k][j-l]==2){
                return 1;
            }
        }
        if(j+1<=n){
            if(arr[k][j+l]==2){
                return 1;
            }
        }
    }
    return 0;
}

void dfs(int n,int c,int s,int l){
    if(n+1==c){
        for(int i=1;i<=n;i++){
            for(int j=1;j<=n;j++){
                printf("%d ",arr[i][j]);
            }
            printf("\n");
        }
        printf("\n");
        ans++;
    }
    else{
        for(int i=s;i<=n;i++){
            for(int j=l;j<=n;j++){
                if(arr[i][j]==0 && check(i,j)==0){
                    arr[i][j]=2;
                    if(i>=n){
                        dfs(n,c+1,1,l+1);
                    }
                    else{
                        dfs(n,c+1,s+1,l);
                    }
                    arr[i][j]=0;
                }
            }
        }
    }
}

int main(){
    scanf("%d",&n);

    if(n==1){
        printf("1");
    }
    else if(n<=3){
        printf("0");
    }
    else{
        dfs(n,1,1,1);
        printf("%d",ans);
    }
    
    return 0;
}
