#include <stdio.h>

int gcd(int a,int b){
    int r;
    r=a%b;
    if(r==0){
        return b;
    }
    else{
        return(gcd(b,r));
    }
}

int main(void){
    int a;
    int b;

    scanf("%d %d",&a,&b);
    
    if(a>b){
        printf("%d",gcd(a,b));
    }
    else{
        printf("%d",gcd(b,a));
    }

    return 0;
}
