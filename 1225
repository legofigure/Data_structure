#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main(){
    char a[10001];
    char b[10001];
    int a_n=0,b_n=0;
    int n,m;
    long long ans=0;
    
    scanf("%s",a);
    scanf("%s",b);
    n=strlen(a);
    m=strlen(b);
    for(int i=0;i<n;i++){
        a_n=(int) a[i];
        for(int j=0;j<m;j++){
            b_n=(int) b[j];
            ans+=(a_n-48)*(b_n-48);
        }
    }

    printf("%lld",ans);
    
    return 0;
}
